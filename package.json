{
  "name": "hv-uikit-react",
  "version": "0.3.0",
  "description": "A collection of React Components for the Hitachi Vantara Design System.",
  "homepage": "https://github.com/pentaho/hv-uikit-react",
  "license": "Apache-2.0",
  "private": true,
  "author": {
    "name": "Hitachi Vantara UI Kit Team"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pentaho/hv-uikit-react.git"
  },
  "scripts": {
    "start": "start-storybook -p 9001",
    "proptypes": "ts-node --skip-project ./scripts/generateProptypes.ts --ignore-cache",
    "clean": "npm-run-all clean:*",
    "clean:package_clean": "npx lerna run clean --parallel",
    "clean:lerna": "npx lerna clean --yes ",
    "bootstrap": "npx lerna bootstrap",
    "dev": "npx lerna run dev --parallel",
    "build": "npx lerna run build",
    "build-documentation": "build-storybook --docs -o dist --quiet",
    "doc": "start-storybook -p 9001 --ci --quiet",
    "test": "npx lerna run test --parallel",
    "test:update": "npx lerna run test:update --parallel",
    "link": "npx lerna run link",
    "generate": "node generator/index.js",
    "publish-": "npx lerna changed && npx lerna publish --yes",
    "publish-prerelease": "npx lerna changed && npx lerna publish prerelease --yes",
    "publish-prepatch": "npx lerna changed && npx lerna publish prepatch --yes",
    "publish-patch": "npx lerna changed && npx lerna publish patch --yes",
    "publish-preminor": "npx lerna changed && npx lerna publish preminor --yes",
    "publish-minor": "npx lerna changed && npx lerna publish minor --yes",
    "publish-premajor": "npx lerna changed && npx lerna publish premajor --yes",
    "publish-major": "npx lerna changed && npx lerna publish major --yes",
    "publish-documentation": "node ./scripts/publish-gh-pages.cjs",
    "lint": "npm-run-all --parallel lint:*",
    "lint:eslint": "npx eslint ./ --cache --ignore-pattern .gitignore --ext .js,.jsx,.ts,.tsx",
    "lint:prettier": "npx lerna run lint:prettier",
    "license-check": "npx lerna run license-check",
    "ts-check": "npx lerna run ts-check --parallel"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.0",
    "@babel/plugin-transform-react-constant-elements": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@material-ui/core": "~4.8.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-docs": "^5.3.19",
    "@storybook/addon-links": "^5.3.19",
    "@storybook/addons": "^5.3.19",
    "@storybook/react": "^5.3.19",
    "@testing-library/dom": "^7.22.2",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^3.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-optimize-clsx": "^2.6.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "chalk": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "find-root": "^1.1.0",
    "fs-extra": "^8.1.0",
    "fs-readdir-recursive": "^1.1.0",
    "gh-pages": "^2.2.0",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lerna": "^3.22.0",
    "license-checker": "^25.0.1",
    "monaco-editor-webpack-plugin": "^1.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "prompt-input": "^3.0.0",
    "react": "^16.13.1",
    "react-docgen": "^5.3.0",
    "react-docgen-deprecation-handler": "^1.1.1",
    "react-dom": "^16.13.1",
    "react-monaco-editor": "^0.36.0",
    "spdx-correct": "^3.1.1",
    "spdx-satisfies": "^5.0.0",
    "ts-node": "^8.10.1",
    "tslint": "5.14.0",
    "typescript": "^3.8.3",
    "typescript-to-proptypes": "^1.5.0"
  },
  "dependencies": {
    "acorn": "^7.2.0",
    "moment": "^2.26.0"
  }
}
